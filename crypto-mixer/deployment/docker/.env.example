# Database Configuration
DB_PASSWORD=your-secure-database-password
REPLICATION_PASSWORD=your-secure-replication-password

# Redis Configuration
REDIS_PASSWORD=your-secure-redis-password

# RabbitMQ Configuration
RABBITMQ_PASSWORD=your-secure-rabbitmq-password

# Kong API Gateway
KONG_DB_PASSWORD=your-secure-kong-db-password

# Application Secrets
JWT_SECRET=your-super-secure-jwt-secret-key
ENCRYPTION_KEY=your-32-character-encryption-key
MASTER_KEY=your-master-key-for-wallets

# HSM Configuration
HSM_PIN=your-hsm-pin
HSM_ENABLED=false

# Blockchain RPC Configuration
BTC_NODE_HOST=your-bitcoin-node-host
