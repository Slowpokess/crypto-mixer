SOL_RPC_URL=https://api.mainnet-beta.solana.com
TRON_API_URL=https://api.trongrid.io

# Monitoring
GRAFANA_PASSWORD=your-secure-grafana-password

# Security Alerts
SLACK_SECURITY_WEBHOOK=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# Environment
NODE_ENV=production